{
  "env": {
    "es6" :true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-undef": "error",
    "prettier/prettier": "error",
    "no-cond-assign": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "eqeqeq": "error",
    "no-alert": "error",
    "semi": ["error", "always"],
    "no-unused-vars": "error"
  }
}